# E2E Testing Environment Variables
# Copy this file to .env.e2e and fill in your values

# Application URL (where the frontend will be running)
VITE_APP_URL=http://localhost:5173

# Backend API URL
VITE_API_URL=http://localhost:5000

# Test User Credentials (create a test user in your system)
E2E_TEST_USER_EMAIL=test@example.com
E2E_TEST_USER_PASSWORD=TestPassword123!

# Optional: Bypass authentication for testing (if implemented)
VITE_BYPASS_AUTH=false